+  # endif
+  # ifdef HAVE_CL_CL_HPP
+  #   include <CL/cl.hpp>
+  # elif defined(HAVE_OPENCL_CL_HPP)
+  #   include <OpenCL/cl.hpp>
+  # else
+  #   error no cl.hpp
+  # endif]],
+                             [[clCreateContextFromType(0,0,0,0,0)]])])
