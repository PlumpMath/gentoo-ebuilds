+  CPPFLAGS=$ax_save_CPPFLAGS
+
+  AC_CHECK_HEADERS([windows.h])
+
+  m4_define([AX_OPENCL_PROGRAM],
+            [AC_LANG_PROGRAM([[
+  # if defined(HAVE_WINDOWS_H) && defined(_WIN32)
