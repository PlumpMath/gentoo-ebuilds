+        if(_ext STREQUAL ".svg")
+            add_custom_command(
+                OUTPUT "${CMAKE_BINARY_DIR}/doc/${_name}.eps"
+                COMMAND ${RSVGCONVERT_EXECUTABLE} ${SVGCONVERT_EPS_FLAGS} --output=${_name}.eps ${CMAKE_CURRENT_SOURCE_DIR}/${_name}.svg
